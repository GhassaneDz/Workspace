package presentation;

import java.util.Iterator;
import java.util.SortedSet;
import java.util.TreeSet;
import domaine.Personne;


public class Lanceur {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// Comment ajouter des elements à une Set et comment on ajoute des éléments au SET
		
		// declarer et initialiser la collection
		SortedSet<Personne> set= new TreeSet<Personne>();
		
		Personne p1 = new Personne(1,"TATA", "Eddy");
		Personne p2 = new Personne(1,"TATA", "Js");
		Personne p3 = new Personne(1,"Roman", "Steven");
		Personne p4 = new Personne(1,"Picard", "Camille");
		
		set.add(p1);
		set.add(p2);
		set.add(p3);
		set.add(p4);
		
		set.comparator();
		
		for (Personne personne : set) {
			
			System.out.println(personne);
			
		}
		



	}

}
